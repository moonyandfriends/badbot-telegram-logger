# Telegram Bot Configuration
logger_telegram_token=your_telegram_bot_token_here

# Supabase Configuration
supabase_url=your_supabase_project_url_here
supabase_key=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_DEBUG=false
LOG_FILE_PATH=logs/telegram_bot.log
LOG_MAX_SIZE=10000000
LOG_BACKUP_COUNT=5

# Database Configuration
MAX_RETRIES=3
RETRY_DELAY=5.0
CONNECTION_TIMEOUT=30

# Backfill Configuration
BACKFILL_ENABLED=true
BACKFILL_CHUNK_SIZE=100
BACKFILL_DELAY_SECONDS=1.0
BACKFILL_MAX_AGE_DAYS=30
BACKFILL_ON_STARTUP=true

# Rate Limiting Configuration
RATE_LIMIT_ENABLED=true
RATE_LIMIT_CALLS=30
RATE_LIMIT_WINDOW=60

# Message Processing Configuration
PROCESS_BOT_MESSAGES=true
PROCESS_SYSTEM_MESSAGES=true
PROCESS_CHANNEL_MESSAGES=true

# Chat and Channel Filtering
ALLOWED_CHATS=
IGNORED_CHATS=
ALLOWED_CHANNELS=
IGNORED_CHANNELS=

# Performance Configuration
BATCH_SIZE=50
FLUSH_INTERVAL=30
MAX_QUEUE_SIZE=10000

# Health Check Configuration
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PORT=8080

# Metrics Configuration
METRICS_ENABLED=false
METRICS_PORT=9090 